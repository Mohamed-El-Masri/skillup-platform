{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=skillup;Integrated Security=True;TrustServerCertificate=True;"
  },
  "JwtSettings": {
    "SecretKey": "your-super-secret-key-here-must-be-at-least-32-characters",
    "Issuer": "SkillUpPlatform",
    "Audience": "SkillUpPlatformAPI",
    "ExpirationInMinutes": 60
  },
  "AIServiceSettings": {
    "BaseUrl": "http://localhost:5000",
    "ApiKey": "your-ai-service-api-key"
  }
}
